{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Bash-Debug (hardcoded script name)",
      "type": "bashdb",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
    },
    {
      "name": "DÃ©boguer un fichier VBS",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/votre_fichier.vbs",
      "runtimeExecutable": "cscript",
      "runtimeArgs": [
          "//nologo"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": [
          "<node_internals>/**"
      ]
    }
  ]
}